  <span class="nx">Pusher</span><span class="p">,</span>
  <span class="nx">PusherMember</span><span class="p">,</span>
  <span class="nx">PusherChannel</span><span class="p">,</span>
  <span class="nx">PusherEvent</span><span class="p">,</span>
<span class="p">}</span> <span class="k">from</span> <span class="dl">'</span><span class="s1">@pusher/pusher-websocket-react-native</span><span class="dl">'</span><span class="p">;</span>

<span class="kd">const</span> <span class="nx">pusher</span> <span class="o">=</span> <span class="nx">Pusher</span><span class="p">.</span><span class="nx">getInstance</span><span class="p">();</span>

  <span class="k">await</span> <span class="nx">pusher</span><span class="p">.</span><span class="nx">init</span><span class="p">({</span>
    <span class="na">apiKey</span><span class="p">:</span> <span class="dl">"</span><span class="s2">1ba4d977d117ecd582b5</span><span class="dl">"</span><span class="p">,</span>
    <span class="na">cluster</span><span class="p">:</span> <span class="dl">"</span><span class="s2">ap2</span><span class="dl">"</span>
  <span class="p">});</span>
    
  <span class="k">await</span> <span class="nx">pusher</span><span class="p">.</span><span class="nx">connect</span><span class="p">();</span>
  <span class="k">await</span> <span class="nx">pusher</span><span class="p">.</span><span class="nx">subscribe</span><span class="p">({</span>
    <span class="na">channelName</span><span class="p">:</span> <span class="dl">"</span><span class="s2">my-channel</span><span class="dl">"</span><span class="p">,</span> 
    <span class="na">onEvent</span><span class="p">:</span> <span class="p">(</span><span class="na">event</span><span class="p">:</span> <span class="nx">PusherEvent</span><span class="p">)</span> <span class="o">=&gt;</span> <span class="p">{</span>
      <span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="s2">`Event received: </span><span class="p">${</span><span class="nx">event</span><span class="p">}</span><span class="s2">`</span><span class="p">);</span>
    <span class="p">}</span>
  <span class="p">});</span>
</code></pre>
          
