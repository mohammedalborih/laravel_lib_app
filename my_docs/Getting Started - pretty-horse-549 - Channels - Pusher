<span class="n">pusher_client</span> <span class="o">=</span> <span class="n">pusher</span><span class="p">.</span><span class="n">Pusher</span><span class="p">(</span>
  <span class="n">app_id</span><span class="o">=</span><span class="s">'1600521'</span><span class="p">,</span>
  <span class="n">key</span><span class="o">=</span><span class="s">'1ba4d977d117ecd582b5'</span><span class="p">,</span>
