  <span class="ss">encrypted: </span><span class="kp">true</span>
<span class="p">)</span>

<span class="n">pusher</span><span class="p">.</span><span class="nf">trigger</span><span class="p">(</span><span class="s1">'my-channel'</span><span class="p">,</span> <span class="s1">'my-event'</span><span class="p">,</span> <span class="p">{</span>
