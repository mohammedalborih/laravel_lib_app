<span class="nb">self</span><span class="p">.</span><span class="nf">pusher</span> <span class="o">=</span> <span class="p">[</span><span class="no">PTPusher</span> <span class="n">pusherWithKey</span><span class="p">:</span><span class="err">@</span><span class="s2">"1ba4d977d117ecd582b5"</span> <span class="n">delegate</span><span class="ss">:self</span> <span class="n">encrypted</span><span class="ss">:YES</span> <span class="n">cluster</span><span class="p">:</span><span class="err">@</span><span class="s2">"ap2"</span><span class="p">];</span>

