  <span class="p">[</span><span class="n">HttpPost</span><span class="p">]</span>
  <span class="k">public</span> <span class="k">async</span> <span class="n">Task</span><span class="p">&lt;</span><span class="n">ActionResult</span><span class="p">&gt;</span> <span class="nf">HelloWorld</span><span class="p">()</span> <span class="p">{</span>
    <span class="kt">var</span> <span class="n">options</span> <span class="p">=</span> <span class="k">new</span> <span class="n">PusherOptions</span>
    <span class="p">{</span>
      <span class="n">Cluster</span> <span class="p">=</span> <span class="s">"ap2"</span><span class="p">,</span>
      <span class="n">Encrypted</span> <span class="p">=</span> <span class="k">true</span>
    <span class="p">};</span>

    <span class="kt">var</span> <span class="n">pusher</span> <span class="p">=</span> <span class="k">new</span> <span class="nf">Pusher</span><span class="p">(</span>
      <span class="s">"1600521"</span><span class="p">,</span>
      <span class="s">"1ba4d977d117ecd582b5"</span><span class="p">,</span>
      <span class="s">"37f314a36b2e18a12ed4"</span><span class="p">,</span>
      <span class="n">options</span><span class="p">);</span>

    <span class="kt">var</span> <span class="n">result</span> <span class="p">=</span> <span class="k">await</span> <span class="n">pusher</span><span class="p">.</span><span class="nf">TriggerAsync</span><span class="p">(</span>
      <span class="s">"my-channel"</span><span class="p">,</span>
