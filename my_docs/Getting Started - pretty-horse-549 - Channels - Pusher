  <span class="n">ssl</span><span class="o">=</span><span class="bp">True</span>
<span class="p">)</span>

<span class="n">pusher_client</span><span class="p">.</span><span class="n">trigger</span><span class="p">(</span><span class="s">'my-channel'</span><span class="p">,</span> <span class="s">'my-event'</span><span class="p">,</span> <span class="p">{</span><span class="s">'message'</span><span class="p">:</span> <span class="s">'hello world'</span><span class="p">})</span>
