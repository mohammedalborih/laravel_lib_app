    <span class="sr">//</span> <span class="n">channelEvent</span><span class="p">.</span><span class="nf">data</span> <span class="n">is</span> <span class="n">a</span> <span class="no">NSDictianary</span> <span class="n">of</span> <span class="n">the</span> <span class="no">JSON</span> <span class="n">object</span> <span class="n">received</span>
