.tooltip-2020--content::after { bottom: calc(var(--spacing-extra-small)*-1); }

.tooltip-2020:focus-within > .tooltip-2020--content, .tooltip-2020:focus > .tooltip-2020--content, .tooltip-2020:hover > .tooltip-2020--content { display: block; }

.tooltip-2020--content { transform: translate(calc(-50% + var(--spacing-medium)*1.25),var(--spacing-extra-small)); }

.tooltip-2020--content.above { transform: translate(calc(-50% + var(--spacing-medium)*1.25),calc(-100% - 30px)); }

svg.delete-icon { overflow: visible; }

.delete-icon > path:first-child { transition: all 0.5s cubic-bezier(0.18, 0.89, 0.32, 1.28) 0s; transform-origin: right bottom; }

.open-trash > path:first-child { transform: rotate(9deg); }

.usage-line { position: relative; padding-bottom: var(--spacing-medium); }

.usage-line-amount, .usage-line-bar { position: absolute; bottom: 0px; content: " "; display: block; height: 0px; padding-bottom: var(--spacing-extra-extra-small); border-radius: var(--border-radius-1); }

.usage-line-bar { width: 100%; background-color: var(--snow); }

.usage-line-amount { transform-origin: left center; animation: 0.4s ease-in-out 0s 1 normal none running load-up-usage-line; transition: width 1s ease-in-out 0s; }

@-webkit-keyframes load-up-usage-line { 
  0% { transform: scaleX(0); }
  100% { transform: scaleX(1); }
}

@keyframes load-up-usage-line { 
  0% { transform: scaleX(0); }
  100% { transform: scaleX(1); }
}

.bg-gradient-home { background: linear-gradient(90deg,var(--dragonfruit-wash),var(--dragonfruit-wash) 50%,var(--white) 0); }

@media screen and (min-width: 40em) {
  .bg-gradient-home-ns { background: linear-gradient(90deg,var(--dragonfruit-wash),var(--dragonfruit-wash) 50%,var(--white) 0); }
